#!/usr/bin/env bash

die() {
    if [[ $# -gt 1 ]]; then
        >&2 echo "$@"
    fi
    exit 1
}

if [[ $# -gt 1 ]]; then
    die "Usage: $0 [FILE]"
fi

declare -A lines
if [[ $# -eq 1 ]]; then
    cat "$1"
else
    cat
fi | while read -r line; do
    if ! [[ "${lines[$line]+abc}" ]]; then
        lines["$line"]=present
        echo "$line"
    fi
done
